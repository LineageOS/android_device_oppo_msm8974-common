on boot
    chown system radio /proc/touchpanel/double_swipe_enable
    chmod 0660 /proc/touchpanel/double_swipe_enable
    chown system radio /proc/touchpanel/up_arrow_enable
    chmod 0660 /proc/touchpanel/up_arrow_enable
    chown system radio /proc/touchpanel/down_arrow_enable
    chmod 0660 /proc/touchpanel/down_arrow_enable
    chown system radio /proc/touchpanel/left_arrow_enable
    chmod 0660 /proc/touchpanel/left_arrow_enable
    chown system radio /proc/touchpanel/right_arrow_enable
    chmod 0660 /proc/touchpanel/right_arrow_enable
    chown system radio /proc/touchpanel/letter_o_enable
    chmod 0660 /proc/touchpanel/letter_o_enable

service vendor.touch-hal-1-0 /vendor/bin/hw/vendor.lineage.touch@1.0-service.oppo_msm8974
    class hal
    user system
    group system
